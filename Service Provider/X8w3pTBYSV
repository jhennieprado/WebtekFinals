$(document).ready(function() {
    for (v in bs) {$('<option>'+v+'</option>').appendTo('#bootstrapVersion')};
    for (v in jq) {$('<option>'+v+'</option>').appendTo('#jqueryVersion')};
    
    
    $("#bootstrapVersion option:contains('3.1.1')").attr('selected', 'selected');
    $("#jqueryVersion option:contains('2.0.2')").attr('selected', 'selected');
    
    
    
    $('.vote-up,.vote-down,.fav').click(function(){
        $('#navLogin').trigger('click');
        return false;
    });
    
    
    $('#desc').click(function(){
        $(this).css({"padding-bottom":"40px"});
    });

});
</script>
